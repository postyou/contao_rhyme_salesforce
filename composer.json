{
    "name": "postyou/contao_rhyme_salesforce_bundle",
    "description": "A Contao CMS extension that integrates with Salesforce",
    "keywords": [
    	"contao",
    	"salesforce",
    	"crm",
    	"force.com"
    ],
    "type": "contao-bundle",
    "license": "LGPL-3.0+",
    "authors": [
        {
            "name": "Rhyme Digital",
            "homepage": "http://rhyme.digital"
        },
        {
            "name": "Postyou Digital",
            "homepage": "https://www.postyou.de"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "contao/core-bundle": "^4.1",
        "postyou/soap-client": "^0.3.*",
        "doctrine/collections": ">=1.3"
    },
    "autoload": {
        "psr-0": {
            "Rhyme\\": [
                "library/"
            ]
        },
        "psr-4": {
            "Postyou\\ContaoRhymeSalesforceBundle\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "Postyou\\ContaoRhymeSalesforceBundle\\ContaoManager\\Plugin"
    },
    "support": {
        "issues": "https://github.com/postyou/contao_rhyme_salesforce_bundle/issues",
        "source": "https://github.com/postyou/contao_rhyme_salesforce_bundle"
    }
}
